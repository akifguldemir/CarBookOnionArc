@model ResultLocationDto

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/UILayout/Index.cshtml";
}

<div class="col-md-12 block-9 mb-md-5">
    <form class="bg-light p-5 contact-form">
        <div class="form-group">
            <input type="text" class="form-control" readonly placeholder="Car">
        </div>
        <div class="form-group">
            <input type="text" class="form-control" placeholder="Your Name">
        </div>
        <div class="form-group">
            <input type="text" class="form-control" placeholder="Your Surname">
        </div>
        <div class="form-group">
            <input type="text" class="form-control" placeholder="Your Email">
        </div>
        <div class="form-group">
            <input type="text" class="form-control" placeholder="Phone Number">
        </div>
        <div class="form-group">
            @Html.DropDownListFor(x => x.LocationID, (List<SelectListItem>)ViewBag.v, new { @class = "form-control", @id = "drop" })
        </div>
        <div class="form-group">
            @Html.DropDownListFor(x => x.LocationID, (List<SelectListItem>)ViewBag.v, new { @class = "form-control", @id = "drop" })
        </div>
        <div class="form-group">
            <input type="text" class="form-control" placeholder="Age">
        </div>
        <div class="form-group">
            <input type="text" class="form-control" placeholder="Licence Year">
        </div>
        <div class="form-group">
            <textarea name="" id="" cols="30" rows="7" class="form-control" placeholder="Message"></textarea>
        </div>
        <div class="form-group">
            <input type="submit" value="Complete Reservation" class="btn btn-primary py-3 px-5">
        </div>
    </form>

</div>


